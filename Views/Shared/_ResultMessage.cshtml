@model AlertMessage
<div class="row mb-4" style="justify-content: center;">
    <div class="container col-md-10">
        <div class="alert" style="background-color: @Model.AlertType;">
            <h4 class="alert-title text-center" style="color:white;">@Model.Title</h4>
            <p class="text-center" style="color:white;">@Model.Message</p>
        </div>
    </div>
</div>